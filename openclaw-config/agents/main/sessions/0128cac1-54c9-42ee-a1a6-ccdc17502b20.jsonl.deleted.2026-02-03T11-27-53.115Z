heme = () => {\n    const newTheme = theme === 'light' ? 'dark' : 'light';\n    setTheme(newTheme);\n    localStorage.setItem('theme', newTheme);\n    document.documentElement.classList.toggle('dark', newTheme === 'dark');\n  };\n\n  if (!mounted) {\n    return null;\n  }\n\n  return (\n    <div className=\"min-h-screen bg-background text-foreground\">\n      {/* Header */}\n      <header className=\"border-b sticky top-0 z-50 bg-background/95 backdrop-blur supports-[backdrop-filter]:bg-background/60\">\n        <div className=\"max-w-7xl mx-auto px-4 sm:px-6 lg:px-8\">\n          <div className=\"flex items-center justify-between h-16\">\n            <div className=\"flex items-center gap-3\">\n              <div className=\"w-10 h-10 rounded-lg bg-primary flex items-center justify-center\">\n                <Code2 className=\"h-6 w-6 text-primary-foreground\" />\n              </div>\n              <div>\n                <h1 className=\"text-xl font-bold\">DevUtils</h1>\n                <p className=\"text-xs text-muted-foreground hidden sm:block\">\n                  Essential developer tools\n                </p>\n              </div>\n            </div>\n            <div className=\"flex items-center gap-2\">\n              <Button\n                variant=\"ghost\"\n                size=\"icon\"\n                onClick={toggleTheme}\n                className=\"rounded-full\"\n                aria-label=\"Toggle theme\"\n              >\n                {theme === 'light' ? (\n                  <Moon className=\"h-5 w-5\" />\n                ) : (\n                  <Sun className=\"h-5 w-5\" />\n                )}\n              </Button>\n              <a\n                href=\"https://github.com\"\n                target=\"_blank\"\n                rel=\"noopener noreferrer\"\n              >\n                <Button variant=\"ghost\" size=\"icon\" className=\"rounded-full\">\n                  <Github className=\"h-5 w-5\" />\n                </Button>\n              </a>\n            </div>\n          </div>\n        </div>\n      </header>\n\n      {/* Main Content */}\n      <main className=\"max-w-7xl mx-auto px-4 sm:px-6 lg:px-8 py-8\">\n        <Tabs defaultValue=\"json\" className=\"w-full\">\n          <TabsList className=\"grid w-full grid-cols-3 sm:grid-cols-6 mb-8\">\n            {tools.map((tool) => (\n              <TabsTrigger\n                key={tool.id}\n                value={tool.id}\n                className=\"flex items-center gap-2\"\n              >\n                <span className=\"text-lg\">{tool.icon}</span>\n                <span className=\"hidden sm:inline\">{tool.label}</span>\n              </TabsTrigger>\n            ))}\n          </TabsList>\n\n          {tools.map((tool) => (\n            <TabsContent key={tool.id} value={tool.id} className=\"mt-0\">\n              {tool.component}\n            </TabsContent>\n          ))}\n        </Tabs>\n      </main>\n\n      {/* Footer */}\n      <footer className=\"border-t mt-auto\">\n        <div className=\"max-w-7xl mx-auto px-4 sm:px-6 lg:px-8 py-6\">\n          <div className=\"flex flex-col sm:flex-row items-center justify-between gap-4\">\n            <p className=\"text-sm text-muted-foreground\">\n              Built with Next.js, Tailwind CSS & shadcn/ui\n            </p>\n            <p className=\"text-sm text-muted-foreground\">\n              All processing happens client-side — your data never leaves your browser\n            </p>\n          </div>\n        </div>\n      </footer>\n    </div>\n  );\n}\n"}],"isError":false,"timestamp":1770114433169}}
